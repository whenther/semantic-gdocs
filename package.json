  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npm-run-all -s lint:*",
    "lint:eslint": "eslint './**/*.{ts,tsx,js}'",
    "lint:prettier": "prettier --check './**/*.{ts,tsx,js,html,md,json,css,yml}'",
    "lint:tsc": "tsc",
    "format": "npm-run-all -s format:*",
    "format:eslint": "eslint --fix './**/*.{ts,tsx,js}'",
    "format:prettier": "prettier --write './**/*.{ts,tsx,js,html,md,json,css,yml}'"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.5",
    "@chakra-ui/react": "^1.3.3",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/jquery": "^3.5.5",
    "@types/js-beautify": "^1.13.1",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "framer-motion": "^3.7.0",
    "jquery": "^3.5.1",
    "js-beautify": "^1.13.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.3.1",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
